{
  "openapi": "3.0.1",
  "info": {
    "title": "LoanOrigination",
    "version": "1.0"
  },
  "paths": {
    "/api/Account/Login/{username}/{pin}": {
      "post": {
        "tags": [
          "Account"
        ],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pin",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/AddCustomer/AddCustomerDetails": {
      "post": {
        "tags": [
          "AddCustomer"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerDetail"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerDetail"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerDetail"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/FindCustomer/customers/search/{firstName}/{lastName}/{dateOfBirth}": {
      "get": {
        "tags": [
          "FindCustomer"
        ],
        "parameters": [
          {
            "name": "firstName",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "lastName",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "dateOfBirth",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CustomerDetail": {
        "required": [
          "address",
          "company_Name",
          "date_of_Birth",
          "email",
          "firstName",
          "last_salary_date",
          "lastName",
          "net_Income",
          "phone",
          "salary"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "firstName": {
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "minLength": 1,
            "type": "string"
          },
          "date_of_Birth": {
            "type": "string",
            "format": "date"
          },
          "phone": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          },
          "address": {
            "minLength": 1,
            "type": "string"
          },
          "company_Name": {
            "minLength": 1,
            "type": "string"
          },
          "salary": {
            "type": "number",
            "format": "double"
          },
          "net_Income": {
            "type": "number",
            "format": "double"
          },
          "last_salary_date": {
            "type": "string",
            "format": "date"
          }
        },
        "additionalProperties": false
      }
    }
  }
}